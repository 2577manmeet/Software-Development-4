var SIZE_ONE = 25;
var SIZE_TWO = 50;

function start() {
	var centerX = getWidth() / 2;
	var centerY = getHeight() / 2;

	drawCircle(SIZE_ONE, centerX, centerY, Color.white);
	drawCircle(SIZE_TWO, centerX, centerY, Color.white);

	drawRectangle(SIZE_ONE, SIZE_TWO, centerX, centerY, Color.white);
	drawRectangle(SIZE_TWO, SIZE_ONE, centerX, centerY, Color.white);

	drawLine(0, 0, centerX, centerY, Color.white);
	drawLine(centerX, 0, 0, centerY, Color.white);

	var img = new WebImage("https://upload.wikimedia.org/wikipedia/commons/9/9a/Gull_portrait_ca_usa.jpg");

	img.setSize(getWidth(),
		getWidth());
	img.setPosition(0, getHeight() / 2 - getWidth() / 2);
	add(img);


}

function drawCircle(radius, cx, cy, color) {
		var circle = new Circle(radius);
		circle.setPosition(cx, cy);
		circle.setColor(color);
		add(circle);
	}

	function drawRectangle(width, height, x, y, color) {
		var rect = new Rectangle(width, height);
		rect.setPosition(x, y);
		rect.setColor(color);
		add(rect);
	}

	function drawLine(startX, startY, endX, endY, color) {
		var line = new Line(startX, startY, endX, endY);
		line.setColor(color);
		add(line);
	}
